    "keywords": [
        "iDEAL",
        "iDEAL 2.0",
        "Currence",
        "Worldline"
    ],
    "autoload": {
        "psr-4": {
            "POM\\iDEAL\\": "src"
        }
    },
    "require": {
        "php": ">= 8.2",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.8",
        "firebase/php-jwt": "^6.10",
        "ramsey/uuid": "^3.9",
        "psr/simple-cache": "^3"
    },
    "require-dev": {
        "matthiasmullie/scrapbook": "^1.5.0"
    }
}
